@font-face
  font-family: 'Bringin'
  src: url('../fonts/Bringin/Bringin.eot')
  src: url('../fonts/Bringin/Bringin.woff2') format('woff2'),
          url('../fonts/Bringin/Bringin.woff') format('woff'),
          url('../fonts/Bringin/Bringin.ttf') format('truetype'),
          url('../fonts/Bringin/Bringin.svg#Bringin') format('svg'),
          url('../fonts/Bringin/Bringin.eot?#iefix') format('embedded-opentype');
  font-weight: normal
  font-style: normal

@font-face
  font-family: "OpenSansRegular"
  src: url("../fonts/OpenSansRegular/OpenSansRegular.eot");
  src: url("../fonts/OpenSansRegular/OpenSansRegular.eot?#iefix")format("embedded-opentype"),
          url("../fonts/OpenSansRegular/OpenSansRegular.woff") format("woff"),
          url("../fonts/OpenSansRegular/OpenSansRegular.ttf") format("truetype");
  font-style: normal
  font-weight: normal


@font-face
  font-family: "OpenSansBold"
  src: url("../fonts/OpenSansBold/OpenSansBold.eot");
  src: url("../fonts/OpenSansBold/OpenSansBold.eot?#iefix")format("embedded-opentype"),
          url("../fonts/OpenSansBold/OpenSansBold.woff") format("woff"),
          url("../fonts/OpenSansBold/OpenSansBold.ttf") format("truetype");
  font-style: normal
  font-weight: normal
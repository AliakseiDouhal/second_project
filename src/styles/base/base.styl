/*!my*/
html
  font-size: 16px

body
  margin: 0
  font-family: OpenSansCondensedLight
  font-weight: normal

h1,h2,h3,h4
  font-family: OpenSansCondensedBold

.content
  max-width: 1440px
  margin: auto
  padding 0 20px
  height 100%

.flex-row-wrap
  flex-flow: row wrap;

.flex-column-center
  flexColumn()
  justify-content: center

.row-space-between
  display flex
  justify-content space-between

.custom-button
  font-weight: 400
  border-width 0
  text-align: center
  white-space: nowrap
  vertical-align: middle
  user-select: none
  transition: background-color .15s ease-in-out, border-color .15s ease-in-out, box-shadow .15s ease-in-out
  border-radius: 4px
  &--blue
    background-color: rgb(54, 174, 234)
    color: white
    &--middle
      width: 253px
      height: 60px
    &--small
      background-color: rgb(54, 174, 234);
      width: 130px;
      height: 40px;
      
.waves
  background url("/img/wave.png") repeat-x
  width: 88px
  height: 7px
  padding-bottom: 40px



